{
  "name": "Custom New Tab",
  "description": "Custom new tab",
  "version": "1.0",
  "chrome_url_overrides": {"newtab": "index.html"},
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "sessions",
    "topSites",
    "history",
    "bookmarks",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["main.js"]
  }],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}